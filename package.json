    "browserslist": {
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ],
        "production": [
            "ie > 6",
            ">1%",
            "not dead",
            "not op_mini all"
        ]
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn run lint",
            "git add --force"
        ],
        "*.{md,json}": [
            "git add --force"
        ]
    },
    "engines": {
        "npm": ">=6.14.6",
        "node": ">=12.18.4"
    },
